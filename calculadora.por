programa {
  funcao inicio() {
    inteiro a,b,soma,sub,div,mult

    escreva("Digite o valor de a: ")
    leia(a)
    escreva("Digite o valor de b ")
    leia(b)
    soma = a+b
    sub = a - b
    div = a / b
    mult = a * b
    escreva("\n Soma = ", soma)
    escreva("\n sub = ", sub)
    escreva("\n div = ", div)
    escreva("\n mult = ", mult)
    

    
  }
}
